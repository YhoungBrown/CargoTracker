{
  "expo": {
    "name": "cargotracker",
    "slug": "cargotracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.expo.cargotracker",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBlbMiCLp-YvL_GgAvnZFweqOzzpUmEWqQ"
        },
        "permissions": [
          "ACCESS_FINE_LOCATION",
          "ACCESS_COARSE_LOCATION"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bca73890-ee7f-4be3-a299-c8f875826a95"
      }
    }
  }
}
